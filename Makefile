	rm -f -r ex6.dSYM
	rm -f ex7
	rm -f -r ex7.dSYM
	rm -f ex8
	rm -f -r ex8.dSYM
	rm -f ex9
	rm -f -r ex9.dSYM
	rm -f ex10
	rm -f -r ex10.dSYM
	rm -f ex11
	rm -f -r ex11.dSYM


